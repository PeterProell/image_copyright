{
	"name": "carsten-walther/image_copyright",
	"type": "typo3-cms-extension",
	"version": "12.4.4",
	"minimum-stability": "stable",
	"description": "Add copyright informations of all images to your site",
	"license": "GPL-3.0-or-later",
	"keywords": [
		"TYPO3",
		"extension",
		"images",
		"copyright"
	],
	"require": {
		"php": "^7.4 || ^8.1 || ^8.2",
		"typo3/cms-core": "^10.4 || ^12.4",
		"typo3/cms-extbase": "^10.4 || ^12.4",
		"typo3/cms-fluid": "^10.4 || ^12.4",
		"typo3/cms-frontend": "^10.4 || ^12.4",
		"typo3/cms-backend": "^10.4 || ^12.4",
		"ext-pdo": "*"
	},
	"authors": [
		{
			"name": "Carsten Walther",
			"role": "Developer",
			"email": "walther.carsten@web.de"
		}
	],
	"autoload": {
		"psr-4": {
			"CarstenWalther\\ImageCopyright\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "image_copyright"
		}
	},
	"replace": {
		"typo3-ter/image_copyright": "self.version"
	}
}
