{
	"name": "walther/image-copytight",
	"type": "typo3-cms-extension",
	"description": "Add copyright informations of all images to your site",
	"keywords": [
		"TYPO3",
		"extension",
		"images",
		"copytight"
	],
	"homepage": "https://carstenwalther.de",
	"license": "GPL-3.0-or-later",
	"require": {
		"typo3/cms-core": "^10.4.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "image_copytight"
		}
	},
	"autoload" : {
		"psr-4" : {
			"Walther\\ImageCopyright\\" : "Classes/"
		}
	},
	"minimum-stability" : "alpha",
	"authors" : [{
		"name" : "Carsten Walther",
		"email" : "walther.carsten@web.de"
	}]
}
